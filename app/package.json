{
  "name": "natla",
  "version": "3.1.0",
  "description": "Sesli sohbet uygulamasÄ±",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SakaGibi/VoiceChat-p2p.git"
  },
  "build": {
    "appId": "com.natla.app",
    "productName": "Natla",
    "publish": [
      {
        "provider": "github",
        "owner": "SakaGibi",
        "repo": "VoiceChat-p2p"
      }
    ],
    "extraResources": [
      "config.json"
    ],
    "asar": true,
    "asarUnpack": [
      "assets/**",
      "assets/*.mp3"
    ],
    "files": [
      "**/*",
      "assets/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/gazmaliyim.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --publish always"
  },
  "keywords": [],
  "author": "SakaGibi",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^5.1.0",
    "electron-updater": "^6.1.7",
    "simple-peer": "^9.11.1"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  }
}